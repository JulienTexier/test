

  <!--================Login Box Area =================-->
  <section class="login_box_area section_gap">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="login_form_inner">
            <h3>Log in to enter</h3>
              <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="row login_form" id="contactForm">
                  <div class="col-md-12 form-group">
                    <%= f.email_field :email, autofocus: true, placeholder: 'Email', autocomplete: "email", class: "form-control" %>
                  </div>

                  <div class="col-md-12 form-group">
                    <%= f.password_field :password, placeholder: 'Password', autocomplete: "current-password", class: "form-control" %>
                  </div>

                  <% if devise_mapping.rememberable? %>
                    <div class="col-md-12 form-group">
                      <div class="creat_account">
                      <%= f.check_box :remember_me %>
                      <%= f.label :remember_me %>
                      </div>
                    </div>
                  <% end %>

                  <div class="col-md-12 form-group">
                    <%= f.submit "Log in", class: "primary-btn" %>
                  </div>
                </div>
              <% end %>

            <%= render "devise/shared/links" %>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!--================End Login Box Area =================-->



